Sort 
{
  = ;
  t.1 e.2, <Partit () t.1 () e.2> : (e.L) t.1 (e.R) = <Sort e.L> t.1 <Sort e.R>;  
}
   
Partit 
{
  (e.L) s.M (e.R) = (e.L) s.M (e.R);
  (e.L) s.M (e.R) s.X e.2, <Compare s.X s.M>:
  { 
    '-' = <Partit (e.L s.X) s.M (e.R) e.2>;
    s.Dummy = <Partit (e.L) s.M (e.R s.X) e.2>;
  };
}