$EXTERN ParseNums;
$EXTERN LOADFILE;

$ENTRY Go
{
    = <Open 'r' 1 <Arg 1>>      
      <Prout <Sort <ParseNums <LOADFILE 1>>>>;
}

Sort 
{
	= ;
	e.1 = <Sort1 () e.1>; 
}
   
Sort1 
{
	() s.X e.1 = <Sort1 (s.X) e.1>;
	(e.1 t.X) t.Y e.2, <Compare t.X t.Y>:
	         { 
	         	'+' = <Sort1 (e.1) t.Y t.X e.2>;
	           	s.Dummy = <Sort1 (e.1 t.X t.Y) e.2>;
	         };
	(e.1) = e.1; 
}